{
    "name": "FindTheFont",
    "version": "1.0",
    "description": "Identifies fonts on any website",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "test.html"
    ]
}